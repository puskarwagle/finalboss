{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "heyguiye",
  "version": "0.1.0",
  "identifier": "com.wagle.heyguiye",
  "build": {
    "devUrl": "http://localhost:1421",
    "beforeDevCommand": "npm run dev"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
    {
      "title": "heyguiye",
      "width": 1800,
      "height": 1200,
      "decorations": true,
      "transparent": false,
      "fullscreen": false,
      "maximized": false,
      "resizable": true
    }
  ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}