{
  "keywords": [
    "input[name=\"q\"]",
    "input#text-input-what"
  ],
  "location": [
    "input[name=\"l\"]",
    "input#text-input-where"
  ],
  "search_button": [
    "button[type=\"submit\"][data-testid*=\"whatWhere\"]",
    "button[type=\"submit\"]"
  ],
  "job_cards": [
    "div.job_seen_beacon",
    "a.tapItem",
    "div.cardOutline"
  ],
  "job_details": {
    "title": "h2.jobsearch-JobInfoHeader-title, h1.jobsearch-JobInfoHeader-title",
