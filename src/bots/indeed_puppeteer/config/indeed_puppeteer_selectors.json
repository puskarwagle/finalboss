    "location": "div.jobsearch-CompanyInfoWithoutHeaderImage div.icl-u-textColor--secondary",
    "description": "#jobDescriptionText, div.jobsearch-jobDescriptionText",
    "salary": "span.salary-snippet, div.attribute_snippet"
  },
  "easy_apply_button": [
    "button:has-text(\"Easily apply\")",
    "button:has-text(\"Quick Apply\")",
    "a:has-text(\"Easily apply\")"
  ],
  "apply_button": [
    "a:has-text(\"Apply on company site\")",
    "a[data-tn-element=\"applyButtonLink\"]"
  ],
  "pagination": {
    "next_page": "a[aria-label=\"Next\"], a[rel=\"next\"]"
  },
  "easy_apply": {
    "resume_select": "select[name=\"resume\"], select[data-testid=\"resume-select\"]",
    "cover_letter_textarea": "textarea[name=\"cover_letter\"], textarea[data-testid=\"cover-letter\"]",
    "submit_button": "button[type=\"submit\"]:has-text(\"Submit\"), button:has-text(\"Apply\")",
    "close_button": "button[aria-label=\"Close\"], button:has-text(\"Cancel\")"
  }
}

