{
  "urls": {
    "home_url": "https://indeed.com",
    "jobs_url": "https://indeed.com/jobs",
    "login_url": "https://secure.indeed.com/auth"
  },
  "auth": {
    "username_input_id": "login-email-input",
    "password_input_id": "login-password-input",
    "signin_button_xpath": "//button[@type='submit']",
    "signin_button_css": "button[type='submit']",
    "signin_link_css": "a[data-tn-element='header-signin-link']",
    "account_menu_css": ".gnav-AccountMenu",
    "user_menu_css": ".np-dropdown",
    "profile_link_css": "a[href*='account']",
    "auth_cookie_names": [
      "__Secure-PassportAuthProxy-BearerToken",
      "__Secure-PassportAuthProxy-RefreshToken",
      "INDEED_CSRF_TOKEN",
      "PPID"
    ]
  },
  "jobs": {
    "job_card_selectors": [
      "div[data-testid='slider_item']",
      ".job_seen_beacon",
      "div[class*='slider_container']",
      "div[class*='result']",
      "div[class*='item']",
      "div[class*='card']",
      "div[class*='job']",
      "div[class*='list']",
      "[data-testid='job-card']",
      ".jobsearch-SerpJobCard",
      "div[class*='jobsearch']",
      "div[class*='serp']",
      "div[class*='jobcard']",
      "div[class*='job-card']",
      "div[class*='result-item']",
      "div[class*='job-item']"
    ],
    "title_selectors": [
      "a.jcs-JobTitle",
      "a[data-jk]",
      "a[class*='job']",
      "h2 a",
      "a[href*='/viewjob']",
      "a[data-testid='job-title']",
      "h3 a"
    ],
    "company_selectors": [
      "[data-testid='company-name']",
      ".company-name",
      "[class*='company']",
      "span[class*='company']"
    ],
    "apply_button_candidates": [
      "#indeedApplyButton",
      "[data-testid='indeedApplyButton-test']",
      "button[aria-label*='Apply now']",
      "button.css-jiauqs",
      "button.e8ju0x50",
      "button:has-text('Apply now')",
      "button:has-text('Apply')",
      "button:has-text('Postuler')",
      "[class*='apply']",
      "button[class*='apply']",
      "button[data-testid='apply-button']",
      "a[data-testid='apply-button']"
    ],
    "indeed_apply_button": "button:has(span[class*='css-1ebo7dz'])",
    "indeed_apply_button_alt": "#indeedApplyButton",
    "apply_button_fallbacks": [
      "button:visible:has-text('Postuler')",
      "button:visible:has-text('Apply')",
      "button:visible:has-text('Quick Apply')",
      "button:visible:has-text('Apply Now')"
    ],
    "external_apply_indicators": [
      "button:has-text('Apply on company site')",
      "button:has-text('Apply on external site')",
      "a[href*='company']",
      "button:has-text('View job')"
    ]
  },
  "application": {
    "resume_card_selector": "[data-testid='FileResumeCardHeader-title']",
    "resume_card_fallbacks": [
      "[data-testid='resume-card']",
      ".resume-card",
      "[class*='resume']",
      "div:has-text('Resume')"
    ],
    "continue_button_selectors": [
      "button:has-text('Continue')",
      "button:has-text('Continuer')",
      "button:has-text('Next')",
      "button:has-text('Suivant')"
    ],
    "submit_button_selectors": [
      "button:has-text('Submit')",
      "button:has-text('Apply')",
      "button:has-text('Postuler')",
      "button:has-text('Déposer ma candidature')",
      "button:has-text('Soumettre')",
      "button:has-text('Bewerben')",
      "button:has-text('Postular')",
      "button:has-text('Submit application')",
      "button:has-text('Send application')"
    ],
    "next_button_selector": "button[type='button']:not([aria-disabled='true']), button[type='submit']:not([aria-disabled='true'])",
    "close_button_selectors": [
      "button[aria-label*='close']",
      "button[aria-label*='cancel']",
      "button[aria-label*='fermer']",
      "button[aria-label*='annuler']",
      "button:has-text('Close')",
      "button:has-text('Cancel')"
    ],
    "application_wizard_selectors": [
      ".application-wizard",
      "[data-testid='application-wizard']",
      ".apply-modal",
      "[class*='application']"
    ],
    "success_indicators": [
      "text='Application submitted'",
      "text='Thank you for applying'",
      "text='Application sent'",
      "text='Candidature envoyée'"
    ]
  },
  "search": {
    "search_input": "input[name='q']",
    "location_input": "input[name='l']",
    "search_button": "button[type='submit']",
    "pagination_next": "a[aria-label='Next Page']",
    "pagination_links": "a[data-testid='pagination-page-link']",
    "results_container": "[data-testid='job-results']",
    "no_results_indicator": "text='No jobs found'"
  },
  "navigation": {
    "page_load_indicator": "body",
    "loading_spinner": ".loading, .spinner, [class*='loading']",
    "error_message": ".error, [class*='error']",
    "cloudflare_challenge": "[data-ray]",
    "captcha_indicator": ".captcha, [class*='captcha']"
  },
  "language_specific": {
    "english": {
      "apply_text": ["Apply", "Quick Apply", "Apply Now"],
      "continue_text": ["Continue", "Next"],
      "submit_text": ["Submit", "Submit application", "Send application"]
    },
    "french": {
      "apply_text": ["Postuler", "Candidature rapide", "Postuler maintenant"],
      "continue_text": ["Continuer", "Suivant"],
      "submit_text": ["Soumettre", "Déposer ma candidature", "Envoyer ma candidature"]
    },
    "german": {
      "apply_text": ["Bewerben", "Schnell bewerben", "Jetzt bewerben"],
      "continue_text": ["Weiter", "Nächste"],
      "submit_text": ["Bewerbung senden", "Absenden"]
    },
    "spanish": {
      "apply_text": ["Postular", "Aplicar", "Postular ahora"],
      "continue_text": ["Continuar", "Siguiente"],
      "submit_text": ["Enviar", "Enviar solicitud"]
    }
  }
}
